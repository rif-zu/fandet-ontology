@prefix : <http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology> .

<http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology> rdf:type owl:Ontology ;
                                                                             rdfs:label "The Fake News Ontology" .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#advertises
:advertises rdf:type owl:ObjectProperty ;
            owl:inverseOf :isAdvertisedBy ;
            rdfs:domain :Social_Media_Agent ;
            rdfs:range :Advert ;
            rdfs:comment """The role advertises refers to the Advert of an Advertiser. It is the inverse of the role isAdvertisedBy.
Note that the advertiser does not need to be the user that created, published or interacted with the content. For instance, an user can interact with some content ADVERT01 posted by an influencer, that contains advertising of a product of the company ADVERTISER01. 
In this case, ADVERTISER01 advertises ADVERT01""" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasChannelAdmin
:hasChannelAdmin rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasRelationTargetUser ;
                 rdfs:domain :ChannelRelation ;
                 rdfs:range :Social_Media_Agent ;
                 rdfs:comment "SubProperty of hasRelationTargetUser that applies when the relation is a ChannelRelation" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasCircus
:hasCircus rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasPart ;
           rdfs:domain :News_Transmission_Event ;
           rdfs:range :SocialMediaRelation ;
           rdfs:comment """Links a News_Transmission_Event to (at most) an SocialMediaRelation, that determines the sort of relationship that the social media user of the News_Transmission_Event had with the context where the associated Social_Media_Object appeared.

Because it is a subrole of hasPart, the inverse relation can be accessed via partOf. That is, 
if:
FNTE01 hasCircus CIR01
then:
CIR01 partOf FNTE01""" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasContent
:hasContent rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasPart ;
            rdfs:domain :News_Transmission_Event ;
            rdfs:range :Social_Media_Object ;
            rdfs:comment """Links a News_Transmission_Event to exactly one Social_Media_Object that determines the object that constitutes the fake news. Because it is a subrole of hasPart, the inverse relation can be accessed via partOf. That is, 
if:
FNTE01 hasContent CON01
then:
CON01 partOf FNTE01""" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasCreatedDate
:hasCreatedDate rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasDateMetadata ;
                rdfs:domain :Social_Media_Object ;
                rdfs:range :Date ;
                rdfs:comment "Links a Social_Media_Object to a Date Metadata_Object that represents the creation date of the content." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasDateMetadata
:hasDateMetadata rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasPart ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :SearchResult
                                             :Social_Media_Object
                                           )
                             ] ;
                 rdfs:range :Date ;
                 rdfs:comment "Links a SearchResult or a Social_Media_Object to a Date Metadata_Object" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasEngagement
:hasEngagement rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasPart ;
               rdfs:domain :News_Transmission_Event ;
               rdfs:range :Engagement ;
               rdfs:comment """Links a News_Transmission_Event to (at most) an Engagement object, that determines the sort of engagement that the social media user of the News_Transmission_Event had with the Social_Media_Object associated with it.
Because it is a subrole of hasPart, the inverse relation can be accessed via partOf. That is, 
if:
FNTE01 hasEngagement EN01
then:
EN01 partOf FNTE01""" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasEntities
:hasEntities rdf:type owl:ObjectProperty ;
             rdfs:domain :Tweet ;
             rdfs:range :Tweet_Entities ;
             rdfs:comment "The role hasEntities links a tweet and tweet entities (e.g., hashtags)." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasGroupMember
:hasGroupMember rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasRelationTargetUser ;
                rdfs:domain :GroupMembershipRelation ;
                rdfs:range :Social_Media_Agent ;
                rdfs:comment "SubProperty of hasRelationTargetUser that applies when the relation is a GroupMemnershipRelation" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasIncludedURL
:hasIncludedURL rdf:type owl:ObjectProperty ;
                rdfs:domain :Tweet_Entities ;
                rdfs:range :IncludedURL ;
                rdfs:comment "The role hasIncludedURL represent URLs included in a social media object" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasMention
:hasMention rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            owl:inverseOf :mentionsUser ;
            rdfs:domain :Social_Media_User ;
            rdfs:range :Mentions ;
            rdfs:comment """The role hasMention refers to a Mention that a Social_Media_User has got. It is the inverse of mentionsUser.

For instance, if USER01 is partOf a FakeNewsTransmissionEvent FNTE01, that an engagement of the type Mention MEN01 which consists on the user mentioning another user USER02, then USER02 hasMention MEN01""" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasPart
:hasPart rdf:type owl:ObjectProperty ;
         owl:inverseOf :partOf ;
         rdfs:domain :NewsConcept ;
         rdfs:range :NewsConcept ;
         rdfs:comment "The hasPart role is interpreted in the standard mereotopological sense. It is the inverse of the partOf role." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasProfileBackground
:hasProfileBackground rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :hasUserImage ;
                      rdfs:domain :Social_Media_User ;
                      rdfs:range :Profile_Background ;
                      rdfs:comment "hasProfileBackground represents  metadata about the user's profile background image." ,
                                   "hashasProfileBackgrounde represents  metadata about the user's profile background image." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasProfileBanner
:hasProfileBanner rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasUserImage ;
                  rdfs:domain :Social_Media_User ;
                  rdfs:range :Profile_Banner ;
                  rdfs:comment "hasProfileBanner represents metadata about the user's profile banner image." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasProfileImage
:hasProfileImage rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasUserImage ;
                 rdfs:domain :Social_Media_User ;
                 rdfs:range :Profile_Image ;
                 rdfs:comment "hasProfileImage represents metadata about the user's profile image." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasPublishedDate
:hasPublishedDate rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasDateMetadata ;
                  rdfs:domain :Social_Media_Object ;
                  rdfs:range :Date ;
                  rdfs:comment "Links a Social_Media_Object to a Date Metadata_Object that represents the publication date of the content." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasPublisher
:hasPublisher rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasPart ;
              rdfs:domain :News_Transmission_Event ;
              rdfs:range :Social_Media_Agent ;
              rdfs:comment """Links a News_Transmission_Event to exactly one Social_Media_Agent, that determines the agent that is generating the transmission of the fake news. Because it is a subrole of hasPart, the inverse relation can be accessed via partOf. That is, 
if:
FNTE01 hasPublisher PUB01
then:
PUB01 partOf FNTE01""" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasRelationOriginUser
:hasRelationOriginUser rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :isRelatedToUser ;
                       rdfs:domain :SocialMediaRelation ;
                       rdfs:range :Social_Media_Agent ;
                       rdfs:comment """Refers to the Social_Media_User from which the SocialMediaRelation originates.

For instance, if USER01 is partOf a News_Transmission_Event FNTE01, with a circus of any type of SocialMediaRelation SMR01 (for instance friendship). then SMR01 hasRelationOriginUser USER01 if USER01 is the user that has a friend.""" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasRelationTargetUser
:hasRelationTargetUser rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :isRelatedToUser ;
                       rdfs:domain :SocialMediaRelation ;
                       rdfs:range :Social_Media_Agent ;
                       rdfs:comment """Refers to the Social_Media_User to which a SocialMediaRelation applies to.

For instance, if USER01 is partOf a News_Transmission_Event FNTE01, with a circus of any type of SocialMediaRelation SMR01 (for instance friendship). then SMR01 hasRelationTargetUser USER02 if USER02 is the friend of USER01 that is the circus of the news.""" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasSearchDate
:hasSearchDate rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasDateMetadata ;
               rdfs:domain :SearchResult ;
               rdfs:range :Date ;
               rdfs:comment "Links a SearchResult to a Date Metadata_Object that represents the date of the search." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasSocialMetadata
:hasSocialMetadata rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasPart ;
                   rdfs:domain :Social_Media_Object ;
                   rdfs:range :Social_Metadata ;
                   rdfs:comment """Links any Social_Media_Object to some SocialMetadata.
Because it is a subrole of hasPart, the inverse relation can be accessed via partOf. That is, 
if:
SMO01 hasMetadata MET01
then:
MET01 partOf SMO01""" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasSocialStats
:hasSocialStats rdf:type owl:ObjectProperty ;
                rdfs:domain :Social_Media_Object ;
                rdfs:range :Social_Stats ;
                rdfs:comment "The role hasSocialStats represents statistical information about social media object (e.g., retweet_count, shares, likes, etc)" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasTweetStats
:hasTweetStats rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasSocialStats ;
               rdfs:domain :Tweet ;
               rdfs:range :Tweet_Stats ;
               rdfs:comment "The role hasTweeStats is particular about Twitter that represents statistical information about a tweet" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasTwitterUserStats
:hasTwitterUserStats rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasUserStats ;
                     rdfs:domain :Twitter_User ;
                     rdfs:range :Twitter_User_Stats .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasUrl
:hasUrl rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :hasSocialMetadata ;
        rdfs:domain :Social_Media_Object ;
        rdfs:range :Url ;
        rdfs:comment """Links any Social_Media_Object to some URL.
Because it is a subrole of hasPart, the inverse relation can be accessed via partOf. That is, 
if:
SMO01 hasUrl URL01
then:
URL01 partOf SMO01""" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasUserImage
:hasUserImage rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasUserMetadata ;
              rdfs:domain :Social_Media_User ;
              rdfs:range :User_Image ;
              rdfs:comment "hasUserImage represents  metadata about the user images (e.g., profile image, background image, etc.)" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasUserMetadata
:hasUserMetadata rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasPart ;
                 rdfs:domain :Social_Media_Object ;
                 rdfs:range :User_Metadata ;
                 rdfs:comment "The role hasUserMetadata links the social media object with user metadata." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasUserStats
:hasUserStats rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :isRelatedToUser ;
              rdfs:domain :Social_Media_Agent ;
              rdfs:range :User_Stats .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#isAdvertisedBy
:isAdvertisedBy rdf:type owl:ObjectProperty ;
                rdfs:domain :Advert ;
                rdfs:range :Advertiser ;
                rdfs:comment """The role isAdvertisedBy refers to the Advertiser of an Advert. It is the inverse of the role advertises.
Note that the advertiser does not need to be the user that created, published or interacted with the content. For instance, an user can interact with some content ADVERT01 posted by an influencer, that contains advertising of a product of the company ADVERTISER01. 
In this case, ADVERT01 isAdvertisedBy ADVERTISER01""" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#isRelatedToInfluencer
:isRelatedToInfluencer rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :isRelatedToUser ;
                       rdfs:domain :Paid_Influencer_Advert ;
                       rdfs:range :Social_Media_Agent ;
                       rdfs:comment "Relation that links those adverts of the class Paid_Influencer_Advert to the Social_Media_Agent that is the influencer of such an advert." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#isRelatedToUser
:isRelatedToUser rdf:type owl:ObjectProperty ;
                 rdfs:domain :NewsConcept ;
                 rdfs:range :Social_Media_User ;
                 rdfs:comment "isRelatedToUser is a high-level property that subsumes diverse properties that link any sort of NewsConcept to a Social_Media_User" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#mentionsUser
:mentionsUser rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :isRelatedToUser ;
              rdfs:domain :Mentions ;
              rdfs:range :Social_Media_User ;
              rdfs:comment """Refers to the Social_Media_User to which a Mentions Engagement is directed. It is the inverse of hasMention.
For instance, if MEN01 involves the action of mentionnung the users USER02 and USER03, then:
(MEN01 tagsUser USER02) and (MEN01 tagsUser USER03)""" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#partOf
:partOf rdf:type owl:ObjectProperty ;
        rdfs:domain :NewsConcept ;
        rdfs:range :NewsConcept ;
        rdfs:comment "The partOf role is interpreted in the standard mereotopological sense. It is the inverse of the hasPart role." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#publishedOn
:publishedOn rdf:type owl:ObjectProperty ;
             rdfs:domain :Advert ;
             rdfs:range :Platform ;
             rdfs:comment "Links an Advert to some Platform where it is published." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#reply_to_tweet
:reply_to_tweet rdf:type owl:ObjectProperty ;
                rdfs:domain :Tweet ;
                rdfs:range :Tweet ;
                rdfs:comment "This role represents a reply to another tweet" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#reply_to_user
:reply_to_user rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :isRelatedToUser ;
               rdfs:domain :Tweet ;
               rdfs:range :Twitter_User .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#tagsUser
:tagsUser rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :isRelatedToUser ;
          rdfs:domain :Tagging ;
          rdfs:range :Social_Media_User ;
          rdfs:comment """Refers to the Social_Media_User to which a Tagging Engagement is directed.
For instance, if TAG01 involves the action of tagging the users USER02 and USER03, then:
(TAG01 tagsUser USER02) and (TAG01 tagsUser USER03)""" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#UID
:UID rdf:type owl:DatatypeProperty ;
     rdfs:domain :NewsConcept ;
     rdfs:range xsd:long ;
     rdfs:comment "Unique ID that can be assigned to any instance of the type NewsConcept" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#coordinates
:coordinates rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :hasSocialMediaObjectProperty ;
             rdfs:domain :Social_Media_Object ;
             rdfs:range xsd:string ;
             rdfs:comment "This property represents the geographic location of this a social media object" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#displayURL
:displayURL rdf:type owl:DatatypeProperty ;
            rdfs:domain :IncludedURL ;
            rdfs:range xsd:string ;
            rdfs:comment "URL of the media to display to clients." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#expandedURL
:expandedURL rdf:type owl:DatatypeProperty ;
             rdfs:domain :IncludedURL ;
             rdfs:range xsd:string ;
             rdfs:comment "An expanded version of display_url. Links to the media display page" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#follow_requests
:follow_requests rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Twitter_User_Stats ;
                 rdfs:comment "The number of request a twitter user sends to other users." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#followers_count
:followers_count rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Twitter_User_Stats ;
                 rdfs:range xsd:long ;
                 rdfs:comment "The number of followers this twitter user currently has." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#friends_count
:friends_count rdf:type owl:DatatypeProperty ;
               rdfs:domain :Twitter_User_Stats ;
               rdfs:range xsd:long ;
               rdfs:comment "The number of users this account is following." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasAge
:hasAge rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf :hasAgentProperty ;
        rdfs:domain :Social_Media_User ;
        rdfs:range xsd:integer ;
        rdfs:comment "Property determining the age of the agent" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasAgentID
:hasAgentID rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :hasAgentProperty ;
            rdfs:domain :Social_Media_Agent ;
            rdfs:range xsd:string ;
            rdfs:comment "The string representation of the unique identifier of a user." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasAgentProperty
:hasAgentProperty rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:comment "High-level property that subsumes the set of properties belonging to a social media agent." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasChannelDescription
:hasChannelDescription rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :hasDescription ;
                       rdfs:domain :ChannelRelation ;
                       rdfs:comment "Description that can be assigned to a ChannelRelation and provides context on the mentioned channel" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasChannelTitle
:hasChannelTitle rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :hasTitle ;
                 rdfs:domain :ChannelRelation ;
                 rdfs:comment "Title of the a ChannelRelation. Note that this not make reference to an object of the class Title, subclass of Social_Metadata. Instead, it contains a string value for a less expensive charactersiation." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasComment
:hasComment rdf:type owl:DatatypeProperty ;
            rdfs:domain :Comment ;
            rdfs:range xsd:string ;
            rdfs:comment "Textual value of a Comment engagement." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasDate
:hasDate rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :hasDateProperty ;
         rdfs:domain :Date ;
         rdfs:comment "Date associated to an object" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasDateOrderFormat
:hasDateOrderFormat rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :hasDateProperty ;
                    rdfs:domain :NumericDate ;
                    rdfs:range [ rdf:type rdfs:Datatype ;
                                 owl:oneOf [ rdf:type rdf:List ;
                                             rdf:first "dmy" ;
                                             rdf:rest [ rdf:type rdf:List ;
                                                        rdf:first "mdy" ;
                                                        rdf:rest [ rdf:type rdf:List ;
                                                                   rdf:first "other" ;
                                                                   rdf:rest [ rdf:type rdf:List ;
                                                                              rdf:first "ydm" ;
                                                                              rdf:rest [ rdf:type rdf:List ;
                                                                                         rdf:first "ymd" ;
                                                                                         rdf:rest rdf:nil
                                                                                       ]
                                                                            ]
                                                                 ]
                                                      ]
                                           ]
                               ] ;
                    rdfs:comment "Format regarding the order of the elements Day, Month and Year. A common format is \"dmy\" and the valid formats are {\"dmy\" , \"mdy\" , \"ydm\" , \"ymd\", \"other\"}." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasDateProperty
:hasDateProperty rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:comment "High-level property that subsumes the set of properties relating to dates." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasDateSeparator
:hasDateSeparator rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :hasDateProperty ;
                  rdfs:domain :NumericDate ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Character used as a separator in a numerical date format. Two common values are \"/\" and \"-\", as in \"01/09/2003\" and \"01-09-2003\"" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasDescription
:hasDescription rdf:type owl:DatatypeProperty ;
                rdfs:domain :NewsConcept ;
                rdfs:range xsd:string ;
                rdfs:comment "Description that can be assigned to any instance of the type News_Concept" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasEmail
:hasEmail rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :hasAgentProperty ;
          rdfs:domain :Social_Media_Agent ;
          rdfs:range xsd:string ;
          rdfs:comment "Property determining the email of the agent" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasFormattedDate
:hasFormattedDate rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :hasDate ;
                  rdfs:range xsd:dateTime ;
                  rdfs:comment "Date in dateTime format." .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasHashtags
:hasHashtags rdf:type owl:DatatypeProperty ;
             rdfs:domain :Tweet_Entities ;
             rdfs:range xsd:string ;
             rdfs:comment "A property which represents hashtags in a tweet" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasID
:hasID rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf :hasSocialMediaObjectProperty ;
       rdfs:domain :Social_Media_Object ;
       rdfs:range xsd:string ;
       rdfs:comment "The string representation of the unique identifier of a social media object" .


###  http://www.semanticweb.org/developer/ontologies/2020/3/Fake_news_Ontology#hasImageFormat
:hasImageFormat rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :hasSocialMediaObjectProperty ;
                rdfs:domain :Image ;
                rdfs:range [ rdf:type rdfs:Datatype ;
                             owl:oneOf [ rdf:type rdf:List ;
                                         rdf:first "GIF" ;
                                         rdf:rest [ rdf:type rdf:List ;
                                                    rdf:first "JPEG" ;
                                                    rdf:rest [ rdf:type rdf:List ;
                                                               rdf:first "PNG" ;
